{
  "name": "s3-cache-buildkite-plugin",
  "version": "1.5.0",
  "description": "Save and restore cache to and from AWS S3.",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/peakon/s3-cache-buildkite-plugin"
  },
  "scripts": {
    "localstack:start": "docker-compose up -d localstack",
    "localstack:wait": "./wait-for-localstack.sh",
    "localstack:stop": "docker-compose down",
    "localstack:setup": "AWS_ACCESS_KEY_ID=dummy AWS_SECRET_ACCESS_KEY=dummy aws s3 mb s3://buildkite-cache --endpoint-url=http://localhost:4566",
    "pretest": "npm run localstack:start && npm run localstack:wait && npm run localstack:setup",
    "posttest": "npm run localstack:stop",
    "test": "LOCALSTACK=true jest",
    "watch": "npm run test -- --watch",
    "lint": "eslint lib",
    "format": "prettier --check --write lib/**/*.js"
  },
  "author": "",
  "license": "ISC",
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "useTabs": false
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 11
    },
    "extends": [
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/**/*.js"
    ]
  },
  "dependencies": {
    "aws-sdk": "^2.675.0",
    "crypto": "^1.0.1",
    "fs-extra": "^9.0.0",
    "meow": "^7.0.1",
    "string-replace-async": "^1.2.1",
    "tar": "^6.0.2"
  },
  "devDependencies": {
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "prettier": "^2.0.5"
  }
}
